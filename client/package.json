{
  "name": "space-is-the-place",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:prod": "vite build",
    "preview": "vite preview",
    "serve:prod": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../.gitignore",
    "format": "prettier --write src/",
    "lh:prod": "lighthouse http://localhost:4173 --output=json --output-path=./lighthouse-prod.json --only-categories=performance,accessibility,best-practices,seo --chrome-flags=\"--headless\""
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "axios": "^1.7.3",
    "pinia": "^2.1.7",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@volar/vue-typescript": "^1.6.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "lighthouse": "^13.0.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^2.2.0"
  }
}
